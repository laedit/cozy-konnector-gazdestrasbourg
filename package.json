{
  "name": "cozy-konnector-gazdestrasbourg",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laedit/cozy-konnector-gazdestrasbourg.git"
  },
  "keywords": [],
  "author": "laedit",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "cozy-run-dev",
    "standalone": "cozy-run-standalone",
    "pretest": "npm run clean",
    "test": "konitor testit .",
    "check": "konitor check .",
    "clean": "rm -rf ./data",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/laedit/cozy-konnector-gazdestrasbourg.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "4.22.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "5.0.4",
    "cozy-app-publish": "0.18.2",
    "cozy-jobs-cli": "1.9.7",
    "eslint": "6.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-config-cozy-app": "1.2.2",
    "git-directory-deploy": "1.5.1",
    "husky": "3.0.4",
    "konitor": "0.10.2",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7"
  }
}
